CREATE TABLE "TRANSACTION" (
	"ID_portfolio"	INTEGER NOT NULL UNIQUE,
	"ABR"	TEXT NOT NULL UNIQUE,
	"price_buy"	INTEGER NOT NULL UNIQUE,
	"quantity"	INTEGER NOT NULL,
	FOREIGN KEY("ABR") REFERENCES "CRYPTO"("ABR"),
	FOREIGN KEY("ID_portfolio") REFERENCES "PORTFOLIO"("ID_portfolio"),
	PRIMARY KEY("price_buy")
)